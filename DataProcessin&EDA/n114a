      "source": [
        "import pandas as pd\n",
        "from math import e  #e ì¸ì‹ ëª¨ë“ˆ\n",
        "from scipy.misc import derivative\n",
        "\n",
        "\n",
        "def num_one(x):\n",
        "    return 4*(x**5) + 9*(x**3) + e*(x**2) - 7 \n",
        "\n",
        "def numerical_derivative(fx, x):\n",
        "    delta_x = 1e-5\n",
        "    return (fx(x + delta_x) - fx(x)) / delta_x\n",
        "\n",
        "res2 = derivative(num_one,df_iris['x'], dx=1e-5).sum()\n",
        "res2= format(res2, \".3f\")\n",
        "res2 "
